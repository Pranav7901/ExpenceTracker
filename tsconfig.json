{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "ESNext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/expencetracking/page.jsx", "app/page.jsx", "utils/db.js", "app/dashboard/page.jsx", "app/components/TransactionForm.jsx", "app/components/dashboard/TransactionList.jsx", "app/components/navbar.jsx", "app/layout.jsx", "app/components/ContactForm.jsx", "app/contact/page.jsx", "app/about/page.jsx"],
  "exclude": ["node_modules"]
}
